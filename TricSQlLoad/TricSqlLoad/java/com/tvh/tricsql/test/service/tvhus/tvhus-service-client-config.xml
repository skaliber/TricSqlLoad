<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:util="http://www.springframework.org/schema/util"
  		xmlns:jaxws="http://cxf.apache.org/jaxws"
  		xmlns:context="http://www.springframework.org/schema/context"
  		  
  	  xsi:schemaLocation="http://www.springframework.org/schema/beans 
  	  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/util 
       http://www.springframework.org/schema/util/spring-util-3.0.xsd
       http://www.springframework.org/schema/context 
       http://www.springframework.org/schema/context/spring-context.xsd
       http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml"/>
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml"/>

	<context:component-scan base-package="com.tvh.blcore.tricsql"></context:component-scan>

	<jaxws:client id="tvhus.defineModelServiceClient" serviceClass="com.tvh.blcore.tricsql.service.extern.tvhus.TVHUSDefineModelService" 
		address="http://localhost:8080/TricSQL/services/TVHUSDefineModel" />
		
	<jaxws:client id="tvhus.catalogServiceServiceClient" serviceClass="com.tvh.blcore.tricsql.service.extern.tvhus.TVHUSCatalogService" 
		address="http://localhost:8080/TricSQL/services/TVHUSCatalogService" />	
	
</beans>     